@model ParkingLotWebApp.Models.ETAs

@{
    ViewBag.Title = WebPages.ETAs_Details;
}
@section pageheader{
    <h1 class="page-header">@WebPages.ETAs <small>@ViewBag.Title</small></h1>
}
@section breadcrumb{
    <li><a href="@Url.Action("Index","Manage")">管理</a></li>
    <li><a href="@Url.Action("Index")">@WebPages.ETAs_Index</a></li>
    <li class="active">@ViewBag.Title</li>
}
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-inverse">
            <div class="panel-heading">
                <div class="panel-heading-btn">
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-click="panel-expand"><i class="fa fa-expand"></i></a>
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-repeat"></i></a>
                </div>
                <h4 class="panel-title">@ViewBag.Title</h4>
            </div>
            <div class="panel-toolbar">
                <div class="btn-group m-r-5" role="group" aria-label="First group">
                    <a href="@Url.Action("Index")" class="btn btn-white" title="@string.Format(Common.Back_To, WebPages.ETAs_Index)"><i class="glyphicon glyphicon-arrow-left"></i></a>
                </div>
                <div class="btn-group m-r-5" role="group" aria-label="Second group">
                    <a href="@Url.Action("Edit", new { id = Model.Id })" class="btn btn-white" title="@Common.Edit"><i class="glyphicon glyphicon-edit"></i></a>
                </div>
            </div>
            <div class="panel-body">
                <div>
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.Code)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Code)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Void)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Void)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.CreateUserId)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.CreateUserId)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.CreateUTCTime)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.CreateUTCTime)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.LastUpdateUserId)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.LastUpdateUserId)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.LastUpdateUTCTime)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.LastUpdateUTCTime)
                        </dd>

                    </dl>
                </div>
            </div>
        </div>
    </div>
</div>
