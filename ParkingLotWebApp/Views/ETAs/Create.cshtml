@model ParkingLotWebApp.Models.ETAs
@using Microsoft.AspNet.Identity
@{
    ViewBag.Title = WebPages.ETAs_Create;
    Model.Void = false;
    Model.CreateUserId = User.Identity.GetUserId<int>();
  Model.CreateUTCTime = DateTime.Now.ToUniversalTime();
}
@section pageheader{
    <h1 class="page-header">@WebPages.ETAs <small>@ViewBag.Title</small></h1>
}
@section breadcrumb{
    <li><a href="@Url.Action("Index","Manage")">管理</a></li>
    <li><a href="@Url.Action("Index")">@WebPages.ETAs_Index</a></li>
    <li class="active">@ViewBag.Title</li>
}
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-inverse">
            <div class="panel-heading">
                <div class="panel-heading-btn">
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-click="panel-expand"><i class="fa fa-expand"></i></a>
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-repeat"></i></a>
                </div>
                <h4 class="panel-title">@ViewBag.Title</h4>
            </div>
            <div class="panel-toolbar">
                <div class="btn-group m-r-5" role="group" aria-label="First group">
                    <a href="@Url.Action("Index")" class="btn btn-white" title="@string.Format(Common.Back_To, WebPages.ETAs_Index)"><i class="glyphicon glyphicon-arrow-left"></i></a>
                </div>
            </div>
            <div class="panel-body">
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="form-horizontal">
                        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                        <div class="form-group">
                            @Html.LabelFor(model => model.Code, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                @Html.EditorFor(model => model.Code, new { htmlAttributes = new { @class = "form-control" } })
                                @Html.ValidationMessageFor(model => model.Code, "", new { @class = "text-danger" })
                            </div>
                        </div>

                        @Html.HiddenFor(model => model.Void)
                        @Html.HiddenFor(model => model.CreateUserId, null)
                        @Html.HiddenFor(model => model.CreateUTCTime, null)
                        @*@Html.EditorFor(model => model.LastUpdateUserId, null)
                @Html.EditorFor(model => model.LastUpdateUTCTime, null)*@
                    </div>
                }
            </div>
            <div class="panel-footer text-right">
                <input type="submit" value="@Common.Create" class="btn btn-success" />
            </div>
        </div>
    </div>
</div>
